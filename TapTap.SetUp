#
# 탭탭이 환경 설정
#

# 탭탭이 기본 에디터
Editor=gvim

# 탭탭이 기동 시 사용할 핫키 지정
# Control, Alt, Shift, F1 ~ F12 등...
Hotkey=Control

# 탭탭이 기동 시, 핫키 두 번 빠르게 누르기
# 핫키 두 번: true
# 핫키 한 번만: false
TapTap=true

# 탭탭이 기동 시, 핫키 두 번 빠르게 누르는 속도 제한(mSec)
# TapTapSpeed=350

# 탭탭이 별칭 입력 상자(Alias Run Box, ARB)의 가로 길이(Pixel)
ArbWidth=400
