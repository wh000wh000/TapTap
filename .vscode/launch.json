    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "ahkdbg",
            "request": "launch",
            "name": "TapTapDebug",
            "program": "${workspaceFolder}\\TapTap.ahk",
            "stopOnEntry": false,
            "captureStreams": true,
            "AhkExecutable": "C:\\App\\Prog\\AutoHotkey\\AutoHotkey.exe",
            "port": 9005
        },
        {
            "type": "ahkdbg",
            "request": "launch",
            "name": "AHKDebug",
            "program": "${file}",
            "stopOnEntry": false,
            "captureStreams": true,
            "AhkExecutable": "C:\\App\\Prog\\AutoHotkey\\AutoHotkey.exe",
            "port": 9005
        },
    ]
}
